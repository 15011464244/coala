<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg"
    android:orientation="vertical" >

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="@dimen/titleSize"
       	android:layout_marginTop="@dimen/connect_info_youjianjieru5_top"
        android:orientation="horizontal"
         >

        <TextView
            android:layout_width="@dimen/titleImgSize"
            android:layout_height="@dimen/titleSize"
            android:background="@drawable/toptext"
            android:layout_marginRight="@dimen/titlepadSize"
            android:gravity="center"
            android:text="@string/mail_connect"
            android:textColor="#ffffff"
            android:textSize="@dimen/btnTextSize" />
        
         <LinearLayout
        	android:layout_width="fill_parent"
        	android:layout_height="@dimen/titleSize"
        	android:orientation="vertical"
        	android:background="@drawable/topright" >
        	<Button
                    android:id="@+id/btncode"
                    android:layout_width="@dimen/erweimasize"
                    android:layout_gravity="right"
                    android:gravity="center"
                    android:layout_height="fill_parent"
                    android:background="@drawable/btndenglu"
                    android:text="@string/code"
                    android:textSize="@dimen/erweimatextsize" />
        </LinearLayout>
    </LinearLayout>
    
    <!--   <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="2dp"
        android:background="@drawable/topline" >
    </LinearLayout>  -->

    <ScrollView
        android:id="@+id/scrollView1"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:fadingEdge="none"  
        android:scrollbarFadeDuration="0"  
        android:gravity="center_horizontal" >
<LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:orientation="vertical" >
        

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="@dimen/connect_info_camera_wigth"
                    android:layout_marginLeft="@dimen/connect_info_mail_no_right"
                    android:layout_marginRight="@dimen/connect_info_mail_no_right"
                    android:layout_marginTop="@dimen/connect_info_youjianjieru5_top"
                    android:background="@drawable/youjianjieru5"
                    android:orientation="horizontal" >

                    <LinearLayout
                        android:layout_width="@dimen/connect_info_camera_wigth"
                        android:layout_height="@dimen/connect_info_camera_height"
                        android:gravity="center_horizontal"
                        android:orientation="vertical" >

                        <Button
                            android:id="@+id/camera"
                            android:layout_width="@dimen/connect_info_camera_wigth"
                            android:layout_height="@dimen/connect_info_camera_height"
                            android:background="@drawable/scan" 
                            android:text="@string/scan"
                            android:textSize="@dimen/connect_info_camera_text_size"
                            android:gravity="bottom|center"
                            android:paddingBottom="@dimen/connect_info_camera_text_btoom"
                            />

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:layout_marginBottom="@dimen/connect_info_mail_no_right"
                        android:layout_marginRight="@dimen/connect_info_mail_no_right"
                        android:layout_marginTop="@dimen/connect_info_mail_no_right"
                        android:orientation="vertical" >

                        <LinearLayout
                 			android:layout_width="fill_parent"
                            android:layout_height="@dimen/connect_info_substitute_fee_height"
                            android:layout_marginTop="@dimen/connect_info_youjianjieru5_top"
                            android:orientation="horizontal" >

                            <LinearLayout
                          		android:layout_width="@dimen/connect_info_ndlvEnid_wigth2"
                                android:layout_height="@dimen/connect_info_ndlvEnid_height"
                                android:layout_marginRight="@dimen/connect_info_mail_no_right"
                                android:orientation="horizontal" >

                                <TextView
                                    android:layout_width="fill_parent"
                                    android:layout_height="fill_parent"
                                    android:gravity="center"
                                    android:text="@string/mail_instance"
                                    android:textSize="@dimen/connect_info_mail_no_size"
                                    android:textColor="#ffffff" />
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="fill_parent"
                              	android:layout_height="@dimen/connect_info_ndlvEnid_height"
                                android:gravity="left"  >

                                <CheckBox
                                    android:id="@+id/substitute_fee"
                                    android:layout_width="wrap_content"
                                    android:layout_height="@dimen/connect_info_substitute_fee_height"
                                    android:button="@drawable/checkboxstyte"
                                    android:checked="false"
                                    android:text="@string/disrepair"
                                    android:textColor="#0163EE"
                                    android:textSize="@dimen/connect_info_substitute_fee_text_size"
                                    android:paddingLeft="@dimen/connect_info_substitute_fee_paddingLeft"
                                     />
                                <!--
                            <CheckBox
                                android:id="@+id/receive_fee"
                                android:layout_width="wrap_content"
                                android:layout_height="50dp"
                                android:button="@drawable/checkboxstyte"
                                android:checked="false"
                                android:text="@string/lose"
                                android:textColor="#0163EE"
                                android:textSize="15dip" />
                                -->
                            </LinearLayout>
                        </LinearLayout>
                        
                        <LinearLayout
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal" >

                            <LinearLayout
                                android:id="@+id/ndlvEnid"
                                android:layout_width="@dimen/connect_info_ndlvEnid_wigth"
                                android:layout_height="@dimen/connect_info_ndlvEnid_height"
                                android:gravity="center"
                                android:layout_marginRight="@dimen/connect_info_mail_no_right"
                                android:orientation="horizontal" >
                                <TextView
                                    android:layout_width="fill_parent"
                                    android:layout_height="fill_parent"
                                    android:gravity="center"
                                    android:text="@string/mail_no"
                                    android:textColor="#ffffff"
                                    android:textSize="@dimen/connect_info_mail_no_size"
                                     />
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="fill_parent"
                                android:layout_height="@dimen/connect_info_ndlvEnid_height"
                                android:gravity="center_vertical"
                                android:orientation="horizontal" >

                                <EditText
                                    android:id="@+id/mailidserch"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:background="@drawable/input2"
                                    android:capitalize="characters"
                                    android:digits="0123456789ABCDEFGHIJKLMNOPQRSTUVWXYZ"
                                    android:focusable="true"
                                    android:hint="@string/scan_hint"
                                    android:textSize="@dimen/connect_info_mailidserch_size"
                                    android:inputType="textCapCharacters"
                                    android:singleLine="true" >
                                    <requestFocus />
                                </EditText>
                            </LinearLayout>
                        </LinearLayout>

                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical" >

                    <LinearLayout
                        android:id="@+id/respid"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="5dip"
                        android:layout_marginLeft="5dip"
                        android:layout_marginRight="5dip"
                        android:layout_marginTop="5dip"
                        android:background="@drawable/youjianjieru5"
                        android:orientation="vertical"
                        android:visibility="gone" >

                        <LinearLayout
                            android:layout_width="fill_parent"
                            android:layout_height="40dip"
                            android:layout_marginBottom="5dip"
                            android:layout_marginRight="10dip"
                            android:layout_marginTop="10dip"
                            android:orientation="horizontal" >

                            <LinearLayout
                                android:layout_width="fill_parent"
                                android:layout_height="40dp"
                                android:layout_weight="5"
                                android:gravity="center"
                                android:orientation="horizontal" >

                                <TextView
                                    android:layout_width="fill_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="2"
                                    android:gravity="center"
                                    android:text="@string/responsible"
                                    android:textColor="#129381" />
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="fill_parent"
                                android:layout_height="fill_parent"
                                android:layout_weight="1.5"
                                android:orientation="horizontal" >

                                <EditText
                                    android:id="@+id/respserch"
                                    android:layout_width="fill_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="2"
                                    android:background="@drawable/input2"
                                    android:capitalize="characters"
                                    android:digits="0123456789ABCDEFGHIJKLMNOPQRSTUVWXYZ"
                                    android:focusable="true"
                                    android:hint="@string/resp_hint"
                                    android:inputType="textCapCharacters"
                                    android:paddingLeft="3dp"
                                    android:paddingRight="3dp"
                                    android:singleLine="true" >

                                    <requestFocus />
                                </EditText>

                                <Button
                                    android:id="@+id/respcamera"
                                    android:layout_width="fill_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="6"
                                    android:background="@drawable/btndenglu"
                                    android:text="@string/scan"
                                    android:textColor="#006699" />
                            </LinearLayout>
                        </LinearLayout>
                        <!--
                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="40dip"
                    android:layout_marginBottom="5dip"
                    android:layout_marginLeft="20dip"
                    android:layout_marginRight="20dip"
                    android:orientation="horizontal" >

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="40dp"
                        android:layout_weight="5"
                        android:background="@drawable/input1"
                        android:orientation="horizontal" >

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="2"
                            android:background="@drawable/input1"
                            android:gravity="center"
                            android:text="@string/responsible_type"
                            android:textColor="#006699" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:layout_weight="1.5"
                        android:orientation="horizontal" >

                        <RadioGroup
                            android:id="@+id/radioGrouplei"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_weight="1.5"
                            android:gravity="center"
                            android:orientation="horizontal" >

                            <RadioButton
                                android:id="@+id/radioin"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/organ"
                                android:textColor="#000000" />

                            <RadioButton
                                android:id="@+id/radioout"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:checked="true"
                                android:text="@string/carrier"
                                android:textColor="#000000" />
                        </RadioGroup>
                    </LinearLayout>
                </LinearLayout>    -->

                        <LinearLayout
                            android:layout_width="fill_parent"
                            android:layout_height="40dip"
                            android:layout_marginBottom="5dip"
                            android:layout_marginRight="10dip"
                            android:orientation="horizontal" >

                            <LinearLayout
                                android:layout_width="fill_parent"
                                android:layout_height="40dp"
                                android:layout_weight="5"
                                android:gravity="center"
                                android:orientation="horizontal" >

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="2"
                                    android:gravity="center"
                                    android:text="@string/abnormity_date"
                                    android:textColor="#129381" />
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="fill_parent"
                                android:layout_height="fill_parent"
                                android:layout_weight="1.5"
                                android:orientation="horizontal" >

                                <EditText
                                    android:id="@+id/reqdate"
                                    android:layout_width="fill_parent"
                                    android:layout_height="wrap_content"
                                    android:background="@drawable/input2"
                                    android:hint="请选择日期 "
                                    android:paddingLeft="3dp"
                                    android:paddingRight="3dp" />
                            </LinearLayout>
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="fill_parent"
                            android:layout_height="50dp"
                            android:layout_marginLeft="20dip"
                            android:layout_weight="1.5"
                            android:orientation="horizontal" >

                            <Button
                                android:id="@+id/imgbutton"
                                android:layout_width="70dp"
                                android:layout_height="fill_parent"
                                android:background="@drawable/tuotou4"
                                android:gravity="bottom|center_horizontal"
                                android:paddingBottom="5dp"
                                android:text="@string/photo"
                                android:textColor="#ffffff" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="20dip"
                            android:layout_marginRight="20dip"
                            android:layout_marginTop="10dip"
                            android:gravity="center"
                            android:orientation="horizontal" >

                            <ImageView
                                android:id="@+id/imageViewcamera"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:src="@android:drawable/alert_dark_frame"
                                android:visibility="gone" />
                        </LinearLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center" >

                        <Button
                            android:id="@+id/okbtn"
                            android:layout_width="@dimen/connect_info_okbtn_width"
                            android:layout_height="@dimen/connect_info_okbtn_height"
                            android:textSize="@dimen/connect_info_beforebtn_textsize"
                            android:layout_marginRight="@dimen/connect_info_okbtn_marginRight"
                            android:background="@drawable/btndenglu"
                            android:text="@string/okbtn" />

                        <Button
                            android:id="@+id/deletebtn"
                            android:layout_width="@dimen/connect_info_deletebtn_width"
                            android:layout_height="@dimen/connect_info_okbtn_height"
                            android:layout_margin="@dimen/connect_info_beforebtn_layout_margin"
                            android:background="@drawable/btndenglu"
                            android:text="@string/scan_delete"
                            android:textSize="@dimen/connect_info_beforebtn_textsize" />

                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical" >

                    <TableLayout
                        android:id="@+id/orderSearchResultTop"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical" >

                        <TableRow
                            android:background="@drawable/libiaotitle"
                            android:gravity="center"
                            android:orientation="horizontal" >

                            <TextView
                                android:id="@+id/displayOrderNum"
                                android:layout_width="wrap_content"
                                android:layout_height="fill_parent"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:text="@string/mail_no"
                                android:textColor="#ffffff"
                                android:textSize="@dimen/connect_info_displayOrderNum_textsize" />

                            <TextView
                                android:id="@+id/displayGoodsName"
                                android:layout_width="wrap_content"
                                android:layout_height="fill_parent"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:text="@string/update_time"
                                android:textColor="#ffffff"
                                android:textSize="@dimen/connect_info_displayOrderNum_textsize" />

                            <TextView
                                android:id="@+id/displayStatus"
                                android:layout_width="wrap_content"
                                android:layout_height="fill_parent"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:text="@string/mail_instance"
                                android:textColor="#ffffff"
                                android:textSize="@dimen/connect_info_displayOrderNum_textsize" />
                        </TableRow>
                    </TableLayout>
                </LinearLayout>

                <ListView
                    android:id="@+id/conListView"
                    android:layout_width="fill_parent"
                    android:layout_height="@dimen/connect_info_conListView_height"
                    android:layout_gravity="left"
                    android:cacheColorHint="#00000000"
                    android:divider="@drawable/line2" />

    <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_alignParentBottom="true"
                    android:gravity="center" >

                    <Button
                        android:id="@+id/beforebtn"
                        android:layout_width="@dimen/connect_info_beforebtn_wigth"
                        android:layout_height="@dimen/connect_info_beforebtn_height"
                        android:layout_margin="@dimen/connect_info_beforebtn_layout_margin"
                        android:background="@drawable/chang"
                         android:layout_marginRight="@dimen/connect_info_okbtn_marginRight"
                        android:text="@string/before"
                        android:textSize="@dimen/connect_info_beforebtn_textsize" />

                    <Button
                        android:id="@+id/nextbtn"
                        android:layout_width="@dimen/connect_info_beforebtn_wigth"
                        android:layout_height="@dimen/connect_info_beforebtn_height"
                        android:layout_margin="@dimen/connect_info_beforebtn_layout_margin"
                        android:background="@drawable/chang"
                        android:text="@string/next"
                        android:textSize="@dimen/connect_info_beforebtn_textsize" />

                </LinearLayout>
                        </LinearLayout>
    </ScrollView>
</LinearLayout>