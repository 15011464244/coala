<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg"
    android:orientation="vertical" >

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="@dimen/titleSize"
        android:layout_marginLeft="@dimen/titlepadSize"
        android:layout_marginTop="@dimen/titlepadSize"
        android:orientation="horizontal"
         >

        <TextView
            android:layout_width="@dimen/titleImgSize"
            android:layout_height="@dimen/titleSize"
            android:background="@drawable/toptext"
            android:layout_marginRight="@dimen/titlepadSize"
            android:gravity="center"
            android:text="@string/exchange_sucdt"
            android:textColor="#ffffff"
            android:textSize="@dimen/btnTextSize" />
        
         <LinearLayout
        	android:layout_width="fill_parent"
        	android:layout_height="@dimen/titleSize"
        	android:layout_marginRight="@dimen/titlepadSize"
        	android:orientation="vertical"
        	android:background="@drawable/topright" >
        	<Button
                    android:id="@+id/btncode"
                    android:layout_width="@dimen/erweiSize"
                    android:layout_gravity="right"
                    android:gravity="center"
                    android:layout_height="@dimen/titleSize"
                    android:background="@drawable/btndenglu"
                    android:text="@string/code"
                    android:textSize="@dimen/btnTextSize" />
        </LinearLayout>
    </LinearLayout>
    
<!--
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="2dp"
        android:background="@drawable/topline" >
    </LinearLayout>
    
    <ScrollView
        android:id="@+id/scrollView1"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:gravity="center_horizontal" > -->

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:orientation="vertical" >

        <LinearLayout
            android:id="@+id/textid"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/titlepadSize"
            android:gravity="center"
            android:orientation="vertical"  >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/imgpadSize"
            	android:layout_marginRight="@dimen/imgpadSize"
                android:text="@string/comp_hint"
                android:textSize="@dimen/imgtitleSize"
                android:textColor="#ff0000" />
            
            <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/line2" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/titlepadSize"
            android:layout_marginLeft="@dimen/imgpadSize"
            android:gravity="center"
            android:orientation="vertical" >

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/out_con"
                    android:textSize="@dimen/btnTextSize"
                    android:textColor="#000000" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=":"
                    android:textSize="@dimen/btnTextSize"
                    android:textColor="#000000" />

                <TextView
                    android:id="@+id/mailcon_out_no"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="FJDD"
                    android:textSize="@dimen/btnTextSize"
                    android:textColor="#000000" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/exchange_time"
                    android:textSize="@dimen/btnTextSize"
                    android:textColor="#000000" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=":"
                    android:textSize="@dimen/btnTextSize"
                    android:textColor="#000000" />

                <TextView
                    android:id="@+id/exchange_time_txt"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="FJDD"
                    android:textSize="@dimen/btnTextSize"
                    android:textColor="#000000" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/mailtotal"
                    android:textSize="@dimen/btnTextSize"
                    android:textColor="#000000" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=":"
                    android:textSize="@dimen/btnTextSize"
                    android:textColor="#000000" />

                <TextView
                    android:id="@+id/mailtotal_txt"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="FJDD"
                    android:textSize="@dimen/btnTextSize"
                    android:textColor="#000000" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/disrepair_no"
                    android:textSize="@dimen/btnTextSize"
                    android:textColor="#000000" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=":"
                    android:textSize="@dimen/btnTextSize"
                    android:textColor="#000000" />

                <TextView
                    android:id="@+id/disrepair_no_txt"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="FJDD"
                    android:textSize="@dimen/btnTextSize"
                    android:textColor="#000000" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:visibility="gone"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/lose_no"
                    android:textSize="@dimen/btnTextSize"
                    android:textColor="#000000" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=":"
                    android:textSize="@dimen/btnTextSize"
                    android:textColor="#000000" />

                <TextView
                    android:id="@+id/lose_no_txt"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="FJDD"
                    android:textSize="@dimen/btnTextSize"
                    android:textColor="#000000" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/line2" />

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_weight="1"
            android:orientation="vertical" >

            <ListView
                android:id="@+id/comListView"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_weight="2"
                android:cacheColorHint="#00000000"
                android:divider="@drawable/line2"
                android:scrollbars="vertical" />

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_weight="8"
                android:gravity="center" >

                <Button
                    android:id="@+id/beforebtn"
                    android:layout_width="fill_parent"
                    android:layout_height="@dimen/btnHSize"
                    android:layout_margin="@dimen/imggapSize"
                    android:layout_weight="1"
                    android:background="@drawable/chang"
                    android:textSize="@dimen/btnTextSize"
                    android:text="@string/re_select" />
                
                <Button
                    android:id="@+id/okbtn"
                    android:layout_width="fill_parent"
                    android:layout_height="@dimen/btnHSize"
                    android:layout_margin="@dimen/imggapSize"
                    android:layout_weight="1"
                    android:background="@drawable/chang"
                    android:textSize="@dimen/btnTextSize"
                    android:text="@string/ok_select" />

                <Button
                    android:id="@+id/nextbtn"
                    android:layout_width="fill_parent"
                    android:layout_height="@dimen/btnHSize"
                    android:layout_margin="@dimen/imggapSize"
                    android:layout_weight="1"
                    android:background="@drawable/chang"
                    android:textSize="@dimen/btnTextSize"
                    android:text="@string/suc_out" /><!-- outform" /> -->
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
    <!-- </ScrollView> -->

</LinearLayout>