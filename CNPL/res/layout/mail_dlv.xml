<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@drawable/bg"
    android:orientation="vertical" >
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="@dimen/titleSize"
       	android:layout_marginTop="@dimen/connect_info_youjianjieru5_top"
        android:orientation="horizontal"
         >

        <TextView
            android:layout_width="@dimen/titleImgSize"
            android:layout_height="@dimen/titleSize"
            android:background="@drawable/toptext"
            android:layout_marginRight="@dimen/titlepadSize"
            android:gravity="center"
            android:text="@string/mail_deliver"
            android:textColor="#ffffff"
            android:textSize="@dimen/btnTextSize" />
        
         <LinearLayout
        	android:layout_width="fill_parent"
        	android:layout_height="@dimen/titleSize"
        	android:orientation="vertical"
        	android:background="@drawable/topright" >
        	<Button
                    android:id="@+id/btncode"
                    android:layout_width="@dimen/erweimasize"
                    android:visibility="gone"
                    android:layout_gravity="right"
                    android:gravity="center"
                    android:layout_height="fill_parent"
                    android:background="@drawable/btndenglu"
                    android:text="@string/code"
                       android:textColor="#ffffff"
                    android:textSize="@dimen/erweimatextsize" />
        </LinearLayout>
    </LinearLayout>
    
    <!--
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="2dp"
        android:background="@drawable/topline" >
    </LinearLayout>
    -->

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_gravity="center"
        android:gravity="center"
        android:orientation="vertical" >

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" >

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/btnHSize"
                android:gravity="center"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/dlv_count"
                    android:textColor="#129381"
                    android:textSize="@dimen/btnTextSize" />

                <TextView
                    android:id="@+id/dlvcount"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="8dp"
                    android:text="0"
                    android:textColor="#129381"
                    android:textSize="@dimen/btnTextSize" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/y_dlv_count"
                    android:textColor="#129381"
                    android:textSize="@dimen/btnTextSize" />

                <TextView
                    android:id="@+id/ydlvcount"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="8dp"
                    android:text="0"
                    android:textColor="#129381"
                    android:textSize="@dimen/btnTextSize" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/n_dlv_count"
                    android:textColor="#129381"
                    android:textSize="@dimen/btnTextSize" />

                <TextView
                    android:id="@+id/ndlvcount"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="0"
                    android:textColor="#129381"
                    android:textSize="@dimen/btnTextSize" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal"
                android:orientation="horizontal" >

                <LinearLayout
                    android:id="@+id/dlvylayout"
                    android:layout_width="@dimen/imgTextSize"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="@dimen/imgpadSize"
                    android:orientation="vertical"
                    android:textSize="@dimen/btnTextSize" >

                    <Button
                        android:id="@+id/dataDownloadBtn"
                        android:layout_width="@dimen/imgTextSize"
                        android:layout_height="@dimen/imgTextSize"
                        android:layout_gravity="left|center"
                        android:background="@drawable/youjiantoudi5"
                        android:gravity="bottom|center_horizontal"
                        android:paddingBottom="@dimen/imggapSize"
                        android:text="@string/data_download"
                        android:textColor="#ffffff"
                        android:textSize="@dimen/btnTextSize" />

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:visibility="gone"
                        android:orientation="horizontal" >

                        <LinearLayout
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:layout_weight="1"
                            android:gravity="right|center_vertical"
                            android:orientation="horizontal" >

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="left|center"
                                android:text="@string/upload"
                                android:textColor="#129381"
                                android:textSize="@dimen/btnTextSize" />
                        </LinearLayout>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="left|center"
                            android:text=":"
                            android:textColor="#129381"
                            android:textSize="@dimen/btnTextSize" />

                        <LinearLayout
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:layout_weight="2"
                            android:gravity="left|center_vertical"
                            android:orientation="horizontal" >

                            <TextView
                                android:id="@+id/dlvuploadText"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="left|center"
                                android:layout_marginLeft="2dp"
                                android:text="0"
                                android:textColor="#129381"
                                android:textSize="@dimen/btnTextSize" />
                        </LinearLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:visibility="gone"
                        android:gravity="center"
                        android:orientation="horizontal" >
					<LinearLayout
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:layout_weight="1"
                            android:gravity="right|center_vertical"
                            android:orientation="horizontal" >
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="left|center"
                            android:layout_marginLeft="@dimen/titlepadSize"
                            android:text="@string/no_upload"
                            android:textColor="#129381"
                            android:textSize="@dimen/btnTextSize" />
					</LinearLayout>
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="left|center"
                            android:text=":"
                            android:textColor="#129381"
                            android:textSize="@dimen/btnTextSize" />
					<LinearLayout
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:layout_weight="2"
                            android:gravity="left|center_vertical"
                            android:orientation="horizontal" >
                        
                        <TextView
                            android:id="@+id/dlvUnUploadText"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="left|center"
                            android:layout_marginLeft="2dp"
                            android:text="0"
                            android:textColor="#129381"
                            android:textSize="@dimen/btnTextSize" />
                    </LinearLayout>
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="@dimen/imgTextSize"
                    android:layout_height="wrap_content"
                    android:orientation="vertical" >

                    <Button
                        android:id="@+id/dlvbuttonok"
                        android:layout_width="@dimen/imgTextSize"
                        android:layout_height="@dimen/imgTextSize"
                        android:layout_gravity="left|center"
                        android:background="@drawable/youjiantoudi1"
                        android:gravity="bottom|center_horizontal"
                        android:paddingBottom="@dimen/imggapSize"
                        android:text="@string/deliver"
                        android:textColor="#ffffff"
                        android:textSize="@dimen/btnTextSize" />

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:visibility="gone"
                        android:orientation="horizontal" >

                        <LinearLayout
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:layout_weight="1"
                            android:gravity="right|center_vertical"
                            android:orientation="horizontal" >

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="left|center"
                                android:text="@string/upload"
                                android:textColor="#129381"
                                android:textSize="@dimen/btnTextSize" />
                        </LinearLayout>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="left|center"
                            android:text=":"
                            android:textColor="#129381"
                            android:textSize="@dimen/btnTextSize" />

                        <LinearLayout
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:layout_weight="2"
                            android:gravity="left|center_vertical"
                            android:orientation="horizontal" >

                            <TextView
                                android:id="@+id/undlvuploadText"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="left|center"
                                android:layout_marginLeft="2dp"
                                android:text="0"
                                android:textColor="#129381"
                                android:textSize="@dimen/btnTextSize" />
                        </LinearLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/btnTextSize"
                        android:gravity="center"
                        android:visibility="gone"
                        android:orientation="horizontal" >

                        <LinearLayout
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:layout_weight="1"
                            android:gravity="right|center_vertical"
                            android:orientation="horizontal" >

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="left|center"
                                android:layout_marginLeft="@dimen/titlepadSize"
                                android:text="@string/no_upload"
                                android:textColor="#129381"
                                android:textSize="@dimen/btnTextSize" />
                        </LinearLayout>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="left|center"
                            android:text=":"
                            android:textColor="#129381"
                            android:textSize="@dimen/btnTextSize" />

                        <LinearLayout
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:layout_weight="2"
                            android:gravity="left|center_vertical"
                            android:orientation="horizontal" >

                            <TextView
                                android:id="@+id/undlvUnUploadText"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="left|center"
                                android:layout_marginLeft="2dp"
                                android:text="0"
                                android:textColor="#129381"
                                android:textSize="@dimen/btnTextSize" />
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:gravity="center_horizontal"
                android:orientation="horizontal" >

                <LinearLayout
                    android:layout_width="@dimen/imgTextSize"
                    android:layout_height="@dimen/imgTextSize"
                    android:layout_marginRight="@dimen/imgpadSize"
                    android:layout_marginTop="@dimen/imgpadSize"
                    android:gravity="left" >

                    <Button
                        android:id="@+id/dlvbuttonn"
                        android:layout_width="@dimen/imgTextSize"
                        android:layout_height="@dimen/imgTextSize"
                        android:layout_gravity="left|center"
                        android:background="@drawable/youjiantoudi2"
                        android:gravity="bottom|center_horizontal"
                        android:paddingBottom="@dimen/imggapSize"
                        android:text="@string/no_deliver"
                        android:textColor="#ffffff"
                        android:textSize="@dimen/btnTextSize" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="@dimen/imgTextSize"
                    android:layout_height="@dimen/imgTextSize"
                    android:layout_marginTop="@dimen/imgpadSize"
                    android:gravity="left" >

                    <Button
                        android:id="@+id/dlvdownload"
                        android:layout_width="@dimen/imgTextSize"
                        android:layout_height="@dimen/imgTextSize"
                        android:layout_gravity="left|center"
                        android:background="@drawable/youjiantoudi3"
                        android:gravity="bottom|center_horizontal"
                        android:paddingBottom="@dimen/imggapSize"
                        android:text="@string/dlv_dianzifandan"
                        android:textColor="#ffffff"
                        android:textSize="@dimen/btnTextSize" />
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>

</LinearLayout>