<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg"
    android:orientation="vertical" >
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="@dimen/titleSize"
       	android:layout_marginTop="@dimen/connect_info_youjianjieru5_top"
        android:orientation="horizontal"
         >

        <TextView
            android:layout_width="@dimen/titleImgSize"
            android:layout_height="@dimen/titleSize"
            android:background="@drawable/toptext"
            android:layout_marginRight="@dimen/titlepadSize"
            android:gravity="center"
            android:text="@string/exchange_sucdt"
            android:textColor="#ffffff"
            android:textSize="@dimen/btnTextSize" />
        
         <LinearLayout
        	android:layout_width="fill_parent"
        	android:layout_height="@dimen/titleSize"
        	android:orientation="vertical"
        	android:background="@drawable/topright" >
        	<Button
                    android:id="@+id/btncode"
                    android:layout_width="@dimen/erweimasize"
                    android:layout_gravity="right"
                    android:gravity="center"
                    android:layout_height="fill_parent"
                    android:background="@drawable/btndenglu"
                    android:text="@string/code"
                    android:textSize="@dimen/erweimatextsize" />
        </LinearLayout>
    </LinearLayout>
    
    <!-- <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="2dp"
        android:background="@drawable/topline" >
    </LinearLayout> -->

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:orientation="vertical" >
        <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical" >
        <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/imggapSize"
                android:orientation="horizontal" >
        	<LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/imgpadSize"
                    android:layout_marginRight="@dimen/imgpadSize"
                    android:gravity="left"
                    android:orientation="horizontal" >

                    <ImageView
                        android:id="@+id/imageViewcamera"
                        android:layout_width="@dimen/btnimgXSize"
                        android:layout_height="@dimen/btnimgXSize"
                        android:src="@android:drawable/alert_dark_frame" />
                </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/imggapSize"
            android:gravity="center"
            android:orientation="vertical" >

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal" >

                <TextView
                    android:id="@+id/codetype"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/mailcon_out"
                    android:textSize="@dimen/btnTextSize"
                    android:textColor="#000000" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=":"
                    android:textSize="@dimen/btnTextSize"
                    android:textColor="#000000" />

                <TextView
                    android:id="@+id/mailcon_out_no"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="FJDD"
                    android:textSize="@dimen/btnTextSize"
                    android:textColor="#000000" />
            </LinearLayout>

            

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal" >

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical" >

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal" >

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/mailtotal"
                            android:textSize="@dimen/btnTextSize"
                            android:textColor="#000000" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text=":"
                            android:textSize="@dimen/btnTextSize"
                            android:textColor="#000000" />

                        <TextView
                            android:id="@+id/mailtotal_txt"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="FJDD"
                            android:textSize="@dimen/btnTextSize"
                            android:textColor="#000000" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal" >

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/disrepair_no"
                            android:textSize="@dimen/btnTextSize"
                            android:textColor="#000000" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text=":"
                            android:textSize="@dimen/btnTextSize"
                            android:textColor="#000000" />

                        <TextView
                            android:id="@+id/disrepair_no_txt"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="FJDD"
                            android:textSize="@dimen/btnTextSize"
                            android:textColor="#000000" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:visibility="gone"
                        android:orientation="horizontal" >

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/lose_no"
                            android:textSize="@dimen/btnTextSize"
                            android:textColor="#000000" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text=":"
                            android:textSize="@dimen/btnTextSize"
                            android:textColor="#000000" />

                        <TextView
                            android:id="@+id/lose_no_txt"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="FJDD"
                            android:textSize="@dimen/btnTextSize"
                            android:textColor="#000000" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal" >

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/upload"
                            android:textSize="@dimen/btnTextSize"
                            android:textColor="#000000" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text=":"
                            android:textSize="@dimen/btnTextSize"
                            android:textColor="#000000" />

                        <TextView
                            android:id="@+id/upload_txt"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="FJDD"
                            android:textSize="@dimen/btnTextSize"
                            android:textColor="#000000" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal" >

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/no_upload"
                            android:textSize="@dimen/btnTextSize"
                            android:textColor="#000000" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text=":"
                            android:textSize="@dimen/btnTextSize"
                            android:textColor="#000000" />

                        <TextView
                            android:id="@+id/no_upload_txt"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="FJDD"
                            android:textSize="@dimen/btnTextSize"
                            android:textColor="#000000" />
                    </LinearLayout>
                </LinearLayout>

            </LinearLayout>
        </LinearLayout>
</LinearLayout>

        <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_marginLeft="@dimen/titlepadSize"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/exchange_time"
                    android:textSize="@dimen/btnTextSize"
                    android:textColor="#000000" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=":"
                    android:textSize="@dimen/btnTextSize"
                    android:textColor="#000000" />

                <TextView
                    android:id="@+id/exchange_time_txt"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="FJDD"
                    android:textSize="@dimen/btnTextSize"
                    android:textColor="#000000" />
            </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/line2" />

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_marginTop="@dimen/imggapSize"
            android:orientation="horizontal" >

            <ListView
                android:id="@+id/comListView"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_gravity="left"
                android:layout_marginTop="@dimen/titlepadSize"
                android:cacheColorHint="#00000000"
                android:divider="@drawable/line2" />
        </LinearLayout>
    </LinearLayout>

</LinearLayout>